<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ICSI418-Group-Project</display-name>
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/QUIZ</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>edu.albany.csi418.session.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>edu.albany.csi418.session.Logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateUser</servlet-name>
    <servlet-class>edu.albany.csi418.user.CreateUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditUser</servlet-name>
    <servlet-class>edu.albany.csi418.user.EditUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateAdmin</servlet-name>
    <servlet-class>edu.albany.csi418.admin.CreateAdmin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditAdmin</servlet-name>
    <servlet-class>edu.albany.csi418.admin.EditAdmin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateQuestionMC</servlet-name>
    <servlet-class>edu.albany.csi418.question.CreateQuestionMC</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditQuestionMC</servlet-name>
    <servlet-class>edu.albany.csi418.question.EditQuestionMC</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateQuestionTF</servlet-name>
    <servlet-class>edu.albany.csi418.question.CreateQuestionTF</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditQuestionTF</servlet-name>
    <servlet-class>edu.albany.csi418.question.EditQuestionTF</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateTest</servlet-name>
    <servlet-class>edu.albany.csi418.test.CreateTest</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditTest</servlet-name>
    <servlet-class>edu.albany.csi418.test.EditTest</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InviteUsers</servlet-name>
    <servlet-class>edu.albany.csi418.test.InviteUsers</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateUser</servlet-name>
    <servlet-class>edu.albany.csi418.user.UpdateUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TakeTest</servlet-name>
    <servlet-class>edu.albany.csi418.test.TakeTest</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestResult</servlet-name>
    <servlet-class>edu.albany.csi418.test.TestResult</servlet-class>
  </servlet>
  <filter>
    <filter-name>userFilter</filter-name>
    <filter-class>edu.albany.csi418.authentication.UserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>userFilter</filter-name>
    <url-pattern>/user/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>adminFilter</filter-name>
    <filter-class>edu.albany.csi418.authentication.AdminFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>adminFilter</filter-name>
    <url-pattern>/admin/*</url-pattern>
  </filter-mapping>
</web-app>
